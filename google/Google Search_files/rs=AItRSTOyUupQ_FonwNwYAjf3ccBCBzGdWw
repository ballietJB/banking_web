var $FOOTABLE = $FOOTABLE || {};
(function( $FOOTABLE, $, undefined ) {

	jQuery.fn.attrAppendWithComma = function (a, c) {
		var b;
		return this.each(function () {
			b = $(this);
			if (b.attr(a) !== undefined && b.attr(a) != "") {
				b.attr(a, b.attr(a) + "," + c);
			} else {
				b.attr(a, c);
			}
		})
	};

	$FOOTABLE.init = function() {
		var $tables;
		$tables = $(".footable");
		$tables.find("th:eq(0)").attr("data-class", "expand");
		$tables.find("th:gt(998)").attrAppendWithComma("data-hide", "tablet");
		$tables.find("th:gt(1)").attrAppendWithComma("data-hide", "phone");
		$tables.footable( { breakpoints: { phone: 320, tablet: 999999 } });

	};
}( $FOOTABLE, jQuery ));

jQuery(function($) {
	$FOOTABLE.init();
